<% provide(:title, current_user.email)%>
<%= render 'devise/shared/error_messages' %>
<div class="row">
  <aside class="col-md-3">
    <%= render 'layouts/user_info' %>
  </aside>
  <div class="col-md-9">
    <div class="center_column">
      <h2>期限が過ぎているTodo</h2>
      <% if @expired_todos.any? %>
        <%= render partial: 'expired_todos', locals: { todos: @expired_todos } %>
      <% else %>
        <p>期限が過ぎているTodoはありません</p>
      <% end %>
    </div>
  </div>
</div>
